  <property name="styleSheet">
   <string notr="true">/* ================================================
   1. GENEL PENCERE AYARLARI
   ================================================ */
QMainWindow, QWidget#centralwidget {
    background-color: #f4f6f9; /* Daha ferah, modern bir kırık beyaz */
    font-family: 'Segoe UI', sans-serif;
}

/* ================================================
   2. SOL MENÜ (Senin kodun + ufak dokunuşlar)
   ================================================ */
#frame_menu {
    background-color: #0b3b29; 
    border-right: 1px solid #082e20;
}

/* SADECE Sol Menüdeki Butonlar İçin Ayarlar */
#frame_menu QPushButton {
    background-color: transparent;
    color: #ecf0f1;
    text-align: left;
    padding-left: 20px;
    height: 45px;
    border: none;
    font-size: 14px;
    font-weight: 500;
    border-left: 4px solid transparent;
}

/* Sol Menü Butonlarına Mouse ile Gelince */
#frame_menu QPushButton:hover {
    background-color: #145a3d;
    color: white;
    border-left: 4px solid #2ecc71; /* Sol taraftaki açık yeşil çizgi */
}

/* Çıkış Butonu Özelleştirmesi */
#btn_cikis {
    margin-top: 20px;
}
#btn_cikis:hover {
    background-color: #8e2a2a;
    border-left: 4px solid #e74c3c;
}

/* ================================================
   3. SAĞ TARAFTAKİ İŞLEM BUTONLARI (Ekle, Sil, Güncelle)
   ================================================ */
/* Menü haricindeki diğer tüm butonlar (Yeşil, kutu şeklinde) */
QPushButton {
    background-color: #145240; /* Senin ana yeşilin */
    color: white;
    border-radius: 6px;
    padding: 8px 15px;
    font-weight: bold;
    font-size: 13px;
    border: 1px solid #0b3b29;
}

QPushButton:hover {
    background-color: #1e7055; /* Üzerine gelince açılan renk */
}

QPushButton:pressed {
    background-color: #0e3b2e; /* Tıklayınca koyulaşan renk */
}

/* Temizle Butonu (İstersen farklı renk yapabilirsin, şimdilik aynı) */
#btn_temizle {
    background-color: #34495e; /* Koyu gri/mavi farklı dursun diye */
}
#btn_temizle:hover {
    background-color: #2c3e50;
}

/* ================================================
   4. KUTUCUKLAR (LINE EDIT) - &quot;Boş görünüyor&quot; demiştin
   ================================================ */
QLineEdit {
    background-color: white;
    border: 1px solid #bdc3c7; /* Gri çerçeve */
    border-radius: 4px;        /* Hafif yuvarlak köşe */
    padding: 6px;              /* Yazı ile kenar arası boşluk */
    color: #2d3436;
    selection-background-color: #145240;
}

QLineEdit:focus {
    border: 2px solid #145240; /* Tıklayınca çerçeve yeşil olsun */
}

/* ================================================
   5. TABLO TASARIMI
   ================================================ */
QTableWidget {
    background-color: white;
    gridline-color: #dfe6e9;
    border: 1px solid #dfe6e9;
    font-size: 13px;
    color: #2d3436;
}

/* Tablo Başlıkları */
QHeaderView::section {
    background-color: #0b3b29; /* Menü ile aynı yeşil */
    color: white;
    padding: 8px;
    border: none;
    font-weight: bold;
}</string>
